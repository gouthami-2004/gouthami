import java.util.Scanner;
public class Factorial {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter Number: ");
        int n = sc.nextInt();
        int result = 1;
        for (int i = 2; i <= n; i++) {
            result=result*i;
        }
        System.out.println("Factorial of " + n + " is: " + result);
    }
}
